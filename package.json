{
  "name": "search-method",
  "displayName": "Search Method",
  "description": "查询方法",
  "version": "0.0.1",
  "publisher": "pwstrick",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:search-method.services"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "search-method.services",
        "title": "Search Services File"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "search-method.services",
          "group": "navigation",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  }
}
